{
    "name": "ibrostudio/laravel-prompts-path-select",
    "description": "Laravel Prompts Path Select",
    "keywords": ["laravel", "prompts"],
    "homepage": "https://github.com/ibrostudio/laravel-prompts-path-select",
    "license": "MIT",
    "authors": [
        {
            "name": "iBroStudio",
            "email": "hello@ibro.studio",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0",
        "laravel/prompts": "^0.3.0",
        "nunomaduro/laravel-console-menu": "^3.5"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "IBroStudio\\PathSelectPrompt\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IBroStudio\\PathSelectPrompt\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -l 8 --memory-limit=1G src tests",
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
